{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "upgrade": {
    "source": "cdn",
    "interval": "168h",
    "auto": false,
    "notice": false
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          },
          "leading_diamond": "\u256d\u2500\ue0b2",
          "template": " {{ if .WSL }}\ue712 on {{ end }}{{ .Icon }}  ",
          "foreground": "#ffffff",
          "background": "#0c7bbb",
          "type": "os",
          "style": "diamond"
        },
        {
          "template": " üè†  ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#DA627D",
          "type": "text",
          "style": "diamond"
        },
        {
          "options": {
            "style": "folder"
          },
          "template": " {{ .Path }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#8a62da",
          "type": "path",
          "style": "powerline"
        },
        {
          "options": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "trailing_diamond": "\ue0b4",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#43CCEA",
          "powerline_symbol": "\ue0b0",
          "background": "#191f48",
          "type": "git",
          "style": "powerline",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "template": "<#cc7eda> | </><#7eb8da>RAM:</> {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB",
          "foreground": "#81ff91",
          "type": "sysinfo",
          "style": "diamond"
        },
        {
          "options": {
            "fetch_version": true
          },
          "template": "<#cc7eda> | </><#7eb8da>\ue718</> {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "foreground": "#81ff91",
          "type": "node",
          "style": "powerline"
        },
        {
          "template": "<#cc7eda> | </><#7eb8da>\ue73d</> {{ .Full }}",
          "foreground": "#81ff91",
          "type": "php",
          "style": "powerline"
        },
        {
          "template": "<#cc7eda> | </><#7eb8da>\ue71e </> {{ .Full }}",
          "foreground": "#81ff91",
          "type": "npm",
          "style": "powerline"
        },
        {
          "type": "python",
          "style": "powerline",
          "foreground": "#81ff91",
          "template": "<#cc7eda> | </><#7eb8da>\ue235</> {{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}",
          "options": {
            "fetch_virtual_env": true,
            "display_mode": "environment",
            "fetch_version": true
          }
        },
        {
          "template": "<> </>",
          "foreground": "#81ff91",
          "type": "sysinfo",
          "style": "powerline"
        },
        {
          "options": {
            "style": "austin",
            "threshold": 150
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": "\u231b  {{ .FormattedMs }} ",
          "foreground": "#4b4b4b",
          "background": "#cecece",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "options": {
            "time_format": "15:04:05"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": "\u23f0  {{ .CurrentDate | date .Format }} ",
          "foreground": "#4b4b4b",
          "background": "#cecece",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u2570\u2500",
          "foreground": "#0c7bbb",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 4,
  "final_space": true
}
